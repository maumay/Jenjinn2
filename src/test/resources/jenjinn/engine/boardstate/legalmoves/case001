white_castle_status: none
black_castle_status: none
developed_pieces: b1 c1 d2 e2 f1 g1 b8 c8 d7 e7 f8 g8
active_side: white
enpassant_square: e5

// Some whitespace separated shorthand chessmoves (potentially spread over multiple lines) or none
//s[a1->a4] c[bq wk] e[c2 c3] s[a1->{d3 c2 e6}]
s[a1 s2] s[a1->d1] s[a3->c1] s[a3->f8] s[b3 b4] s[c3->{a2 a4 b5 d5 e4 e2 d1 b1}]
